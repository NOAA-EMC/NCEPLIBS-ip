# @configure_input@

# Top level makefile template

MAKE="gmake"

# Target specification
all clean install uninstall:
	cd copygb/src && $(MAKE) $@
	cd ipxwafs/src && $(MAKE) $@
	cd gausslat/src && $(MAKE) $@
	cd gdswiz_wzd/src && $(MAKE) $@
	cd ipxetas/src && $(MAKE) $@
	cd ipmerge2/src && $(MAKE) $@
	cd makgds/src && $(MAKE) $@
	cd ipolates/src && $(MAKE) $@
	cd ipolatev/src && $(MAKE) $@

distclean:
	cd copygb/src && $(MAKE) $@
	cd ipxwafs/src && $(MAKE) $@
	cd gausslat/src && $(MAKE) $@
	cd gdswiz_wzd/src && $(MAKE) $@
	cd ipxetas/src && $(MAKE) $@
	cd ipmerge2/src && $(MAKE) $@
	cd makgds/src && $(MAKE) $@
	cd ipolates/src && $(MAKE) $@
	cd ipolatev/src && $(MAKE) $@
	-rm Makefile config*.log config.status

Makefile: Makefile.in config.status
	./config.status $@

config.status: configure
	./config.status --recheck

.PHONY: all clean distclean check install uninstall
